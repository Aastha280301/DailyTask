<%= form_with model: @semester do |form| %>
  <div>
    <%= form.label :course_id %><br>
    <%= form.text_field :course_id %>
  </div>
    <div>
    <%= form.label :semester_name %><br>
    <%= form.select :semester_name, ["1", "2", "3", "4", "5" ,"6", "7", "8"] %>

  </div>
  <div>
    <%= form.submit "Submit"%>
  </div>
<% end %>